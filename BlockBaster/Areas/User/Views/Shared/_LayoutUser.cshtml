<html lang="en" class="no-js">


<head>
	<!-- Basic need -->
	@await Html.PartialAsync("MetaTagsPartial")
	@await Html.PartialAsync("CSSPartial")

    <!--Google Font-->
    @await Html.PartialAsync("GoogleFontsPartial")
	<!-- Mobile specific meta -->
	@await Html.PartialAsync("MobileSpecificMetaPartial")

</head>
<body>
<!--preloading-->
@await Html.PartialAsync("PreloaderPartial")
<!--end of preloading-->
<!--login form popup-->

<!-- BEGIN | Header -->
@await Html.PartialAsync("HeaderUser")
<!-- END | Header -->

@RenderBody()


<!-- footer section-->

@await Html.PartialAsync("FooterPartial")
<!-- end of footer section-->
@await Html.PartialAsync("ScriptsPartial")

</body>


</html>
